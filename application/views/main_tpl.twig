<!doctype html>
<html class="no-js" lang="en">
<head>
  <meta charset="UTF-8">
  <title>{{ title }}</title>

  {#<meta name="viewport" content="initial-scale=1">#}
  <meta name="viewport" content="width=device-width">

  <meta name="description" content="dota replay parser">
  <meta name="keywords" content="warcraft,dota,games,savereplay{{ keywords }}">

  <link type="text/plain" rel="author" href="{{ url }}humans.txt" />

  {% for style in styles %}
    <link type="text/css" rel="stylesheet" href="{{ style }}" />
  {% endfor %}

</head>
<body {% if body_class %} class="{{ body_class }}" {% endif %}>


  {{ adminmenu|raw }}

  <div class="page">
    {% if user %}
      {% include 'panel/user_head.twig' with user %}
    {% endif %}

    <header>
      <a class="logo" href="{{ url }}">w3xSilverCloud</a>

      {% if nav %}
        {{ nav|raw }}
      {% endif %}

    </header> <!-- header -->

    {% if message %}
      {{ message|raw }}
    {% endif %}

    {% if submenu %}
      {{ submenu|raw }}
    {% endif %}

    <div class="page-holder donation-wrapper">
      {# donate #}
      <form action="https://www.paypal.com/cgi-bin/webscr" method="post" target="_top">
        <span>
          Make donation to keep service alive:
        </span>
        <input type="hidden" name="cmd" value="_s-xclick">
        <input type="hidden" name="hosted_button_id" value="FB4SZ9SJM76U2">
        <input type="image" src="http://w3xsilvercloud.com/pp_donate.png" border="0" name="submit" alt="PayPal - The safer, easier way to pay online!">
        <img alt="" border="0" src="https://www.paypalobjects.com/en_US/i/scr/pixel.gif" width="1" height="1">

      </form>

    </div>

    <div class="region-content">

      {% if pagetitle %}
        <h1 class='page-title'>{{ pagetitle }}</h1>
      {% endif %}

      {{ content|raw }}
    </div> <!-- region-content -->
    <footer class="region-footer">
      <div class="footer-inner">
        <p>copyright {{ "now"|date("Y") }} by <a target="_blank" href="http://webpage-lab.com">Webpage-lab.com</a></p>
    {# {{ memory_usage }} / {{ elapsed_time }} #}
      </div>
    </footer> <!-- footer -->

  </div> <!-- page -->

  <div id="script" style="display: none; visibility: hidden;">
    <script type="text/javascript" src="http://code.jquery.com/jquery-2.0.2.min.js"></script>
    <script>
      $('html').removeClass('no-js').addClass('js');
    </script>
    {% if jsplugin %}
      {% for js in jsplugin %}
        <script type="text/javascript" src="{{ js }}"></script>
      {% endfor %}
    {% endif %}
    <script type="text/javascript" src="/script/api.js"></script>
    {{ analitics|raw }}
  </div>



</body>
</html>